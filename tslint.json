{
	"rules": {
		"no-string-throw": true,
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"curly": true,
		"class-name": true,
		"semicolon": [
			true,
			"always"
		],
		"triple-equals": true,
		"no-any": true,
		"typedef":  [true, "call-signature"]
	},
	"defaultSeverity": "warning"
}
